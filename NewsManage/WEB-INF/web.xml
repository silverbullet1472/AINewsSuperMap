    <servlet-name>loadData</servlet-name>
    <url-pattern>/loadData</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>updateImage</servlet-name>
    <url-pattern>/updateImage</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>loadChart</servlet-name>
    <url-pattern>/loadChart</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>addData</servlet-name>
    <url-pattern>/addData</url-pattern>
  </servlet-mapping>
    
<welcome-file-list>
        
<welcome-file>login.jsp</welcome-file>
    
</welcome-file-list>

</web-app>